<?xml version="1.0" encoding="UTF-8" ?>

<persistence xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd" version="2.0">

    <persistence-unit name="PosWeb">
		
		<provider>org.datanucleus.api.jpa.PersistenceProviderImpl</provider>
		
		<class>com.app.posweb.server.model.Sync</class>
		<class>com.app.posweb.server.model.Merchant</class>
		<class>com.app.posweb.server.model.ProductGroup</class>
		<class>com.app.posweb.server.model.Product</class>
		<class>com.app.posweb.server.model.Discount</class>
		<class>com.app.posweb.server.model.Employee</class>
		<class>com.app.posweb.server.model.Customer</class>
		<class>com.app.posweb.server.model.Cashflow</class>
		<class>com.app.posweb.server.model.User</class>
		<class>com.app.posweb.server.model.Transactions</class>
		<class>com.app.posweb.server.model.TransactionItem</class>
		<class>com.app.posweb.server.model.Supplier</class>
		<class>com.app.posweb.server.model.Bills</class>
		<class>com.app.posweb.server.model.Inventory</class>
		<class>com.app.posweb.server.model.MerchantAccess</class>
		<class>com.app.posweb.server.model.UserAccess</class>
		<class>com.app.posweb.server.model.Orders</class>
		<class>com.app.posweb.server.model.OrderItem</class>
		
		<exclude-unlisted-classes>true</exclude-unlisted-classes>
		
		<properties>
		    <property name="javax.persistence.jdbc.user" value="root"/>
			<property name="javax.persistence.jdbc.password" value="enggar"/>
			<property name="datanucleus.identifier.case" value="LowerCase"/>
		</properties>

	</persistence-unit>
	
</persistence>
